{
  "name": "ecr-viewer",
  "packageManager": "^npm@10.8.1",
  "description": "The DIBBs eCR Viewer app offers a REST API for processing FHIR messages into an HTML page with key insights.",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "local-dev": "npx @dotenvx/dotenvx run -f .env.local -- sh -c 'docker compose -f ./docker-compose.yaml --profile ${CONFIG_NAME} up -d && npm run dev'",
    "local-docker": "npx @dotenvx/dotenvx run -f .env.local -- sh -c 'docker compose -f ./docker-compose.yaml --profile ${CONFIG_NAME} --profile ecr-viewer up'",
    "local-docker:build": "npx @dotenvx/dotenvx run -f .env.local -- sh -c 'docker compose -f ./docker-compose.yaml --profile ${CONFIG_NAME} --profile ecr-viewer up --build'",
    "local-docker:silent": "npx @dotenvx/dotenvx run -f .env.local -- sh -c 'docker compose -f ./docker-compose.yaml --profile ${CONFIG_NAME} --profile ecr-viewer up -d'",
    "setup-local-env": "../../setup-env.sh .env.local",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write '**/*.{js,json,md,ts,tsx,scss,yaml,yml}'",
    "format:check": "prettier --check '**/*.{js,json,md,ts,tsx,scss,yaml,yml}'",
    "test": "TZ=America/New_York jest",
    "test:watch": "TZ=America/New_York jest --watch",
    "test:e2e": "npx playwright test",
    "clear-local": "docker compose -f ./seed-scripts/docker-compose-seed.yaml --profile \"*\" down -v",
    "convert-seed-data": "npx @dotenvx/dotenvx run -f .env.local -- sh -c 'docker compose -f ./seed-scripts/docker-compose-seed.yaml --profile ${CONFIG_NAME} --profile ecr-viewer up --abort-on-container-exit'",
    "convert-seed-data:build": "npx @dotenvx/dotenvx run -f .env.local -- sh -c 'docker compose -f ./seed-scripts/docker-compose-seed.yaml --profile ${CONFIG_NAME} --profile ecr-viewer up --abort-on-container-exit --build'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.682.0",
    "@azure/storage-blob": "^12.24.0",
    "@next/bundle-analyzer": "^15.1.7",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.52.0",
    "@opentelemetry/exporter-jaeger": "^1.27.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.54.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.54.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.54.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.54.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.54.0",
    "@opentelemetry/propagation-utils": "^0.30.12",
    "@opentelemetry/propagator-aws-xray": "^1.26.0",
    "@opentelemetry/redis-common": "^0.36.2",
    "@opentelemetry/resource-detector-aws": "^1.7.0",
    "@opentelemetry/resource-detector-azure": "^0.2.12",
    "@opentelemetry/resource-detector-container": "^0.5.0",
    "@opentelemetry/resource-detector-gcp": "^0.29.13",
    "@opentelemetry/resources": "^1.27.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@opentelemetry/sdk-trace-node": "^1.27.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@opentelemetry/sql-common": "^0.40.1",
    "@opentelemetry/winston-transport": "^0.7.0",
    "@testing-library/user-event": "^14.5.2",
    "@trussworks/react-uswds": "^9.1.0",
    "@vercel/otel": "^1.10.0",
    "classnames": "^2.5.1",
    "date-fns": "^3.4.0",
    "fhirpath": "^3.10.4",
    "html-react-parser": "^5.1.4",
    "jose": "5.2.2",
    "js-yaml": "4.1.0",
    "mssql": "^11.0.1",
    "next": "^14.2.14",
    "next-auth": "^4.24.7",
    "next-router-mock": "^0.9.13",
    "next-runtime-env": "^3.2.2",
    "node-html-parser": "^7.0.1",
    "pg-promise": "^11.6.0",
    "react": "^18",
    "react-dom": "^18",
    "sanitize-html": "^2.13.1",
    "server-only": "^0.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@dotenvx/dotenvx": "^1.25.1",
    "@next/eslint-plugin-next": "^14.2.5",
    "@playwright/test": "^1.50.0",
    "@smithy/util-stream": "^2.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^15.0.7",
    "@types/fhir": "^0.0.40",
    "@types/jest-axe": "^3.5.9",
    "@types/js-yaml": "^4.0.9",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/sanitize-html": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "autoprefixer": "^10.4.19",
    "aws-sdk-client-mock": "^3.1.0",
    "concurrently": "^9.0.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-fail-on-console": "^3.3.1",
    "jest-fetch-mock": "^3.0.3",
    "postcss": "^8.4.38",
    "prettier": "3.1.0",
    "sass": "^1.69.5",
    "typescript": "^5.5.2"
  }
}
